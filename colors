class Colors:
    cinza_escuro = (26, 31, 40)
    verde = (47, 230, 23)
    vermelho = (232, 18, 18)
    laranja = (226, 116, 15)
    amarelo = (237, 233, 4)
    roxo = (166, 0, 247)
    azul_ciano = (21, 204, 209)
    azul = (17, 65, 216)

    @classmethod
    def cor_das_malhas(cor):
        return [cor.cinza_escuro, cor.verde, cor.vermelho, cor.laranja, cor.amarelo, cor.roxo, cor.azul_ciano, cor.azul]
